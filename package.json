  },
  "homepage": "https://github.com/conveyal/gridualizer#readme",
  "dependencies": {
    "d3-color": "^1.0.3",
    "d3-scale": "^1.0.6",
    "lodash": "^4.17.4",
    "simple-statistics": "4.1.0",
    "xorshift": "^1.1.0"
  },
  "devDependencies": {
    "browsochrones": "^1.0.0",
    "leaflet": "^1.1.0",
    "mastarm": "^3.11.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
